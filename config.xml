<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CogniTest</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="kiran.chandrasekhar03@gmail.com">
        Kiran Chandrasekhar
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="mailto:*" launch-external="yes"/>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/img/icons/icon-36-ldpi.png" density="ldpi"/>
        <icon src="www/img/icons/icon-48-mdpi.png" density="mdpi"/>
        <icon src="www/img/icons/icon-72-hdpi.png" density="hdpi"/>
        <icon src="www/img/icons/icon-96-xhdpi.png" density="xhdpi"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="www/img/icons/icon-36-ldpi.png" density="ldpi"/>
        <icon src="www/img/icons/icon-48-mdpi.png" density="mdpi"/>
        <icon src="www/img/icons/icon-72-hdpi.png" density="hdpi"/>
        <icon src="www/img/icons/icon-96-xhdpi.png" density="xhdpi"/>
    </platform>
</widget>
